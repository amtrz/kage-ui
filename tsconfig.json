{
  "compilerOptions": {
    "module": "ES2022",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "lib": ["DOM", "ESNext", "DOM.Iterable"],
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "verbatimModuleSyntax": true
  }
}
